{
  "name": "protractor-js-automation",
  "description": "Template to Test Project that contains Protractor tests",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "repository": "ssh://git@stash.cognifide.com/qa/protractor-template.git",
  "scripts": {
    "postinstall": "npm run webdriver-manager-update",
    "test": "protractor ./protractor-conf.js",
    "test:reportTests": "protractor ./protractor-conf.js --suite=reportTests",
    "test:apiTests": "protractor ./protractor-conf.js --suite=apiTests",
    "webdriver-manager-start": "webdriver-manager start",
    "webdriver-manager-update": "webdriver-manager update"
  },
  "devDependencies": {
    "atob": "^2.0.3",
    "chakram": "^1.5.0",
    "jasmine-json-test-reporter": "^1.0.0-beta",
    "jasmine-reporters": "^2.2.1",
    "jasmine-spec-reporter": "^3.2.0",
    "jasmine-terminal-reporter": "^1.0.3",
    "protractor": "^5.1.1",
    "protractor-jasmine2-screenshot-reporter": "^0.3.3",
    "sg-protractor-tools": "^1.0.4",
    "webdriver-manager": "^12.0.4"
  },
  "engines": {
    "node": "6.9.2"
  }
}
